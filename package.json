{
  "name": "csgo-float",
  "version": "0.1.0",
  "description": "CS:GO Float values",
  "main": "dist",
  "scripts": {
    "start": "babel-node example",
    "build": "rm -rf dist && mkdir dist && babel src -o dist/index.js",
    "lint": "eslint src example.js protos/*.js",
    "protos": "babel-node protos/update"
